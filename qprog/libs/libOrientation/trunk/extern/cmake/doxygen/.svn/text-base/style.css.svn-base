/* Generated by KompoZer */
body {
  margin: 0;
  padding: 0;
  background: #eeeedd none repeat scroll 0% 50%;
  text-align: center;
  font-size: 80%;
  font-family: Geneva,Arial,Georgia,sans-serif;
  line-height: 1.4em;
}
h1, h2, h3, h4, h5, h6 {
  font-family: Georgia,Geneva,Arial,times,serif;
  letter-spacing: 0.09em;
  line-height: 1.6em;
}
li {
  margin: 0;
  padding: 0.3em 0;
}
blockquote {
  border: 1px solid #ccccbb;
  margin: 1em;
  padding: 10px;
  background: #ddddcc none repeat scroll 0% 50%;
}
abbr {
  border-bottom: 1px solid #000000;
}
a {
  border:  none;
  background: transparent none repeat scroll 0% 50%;
  text-decoration: none;
}
a:link, a:visited {
  color: #b23333;
}
a:hover, a:active {
  border-bottom: 1px solid #666666;
  color: #333333;
}

#header {
  margin: 0;
  padding: 0;
  background: #fdd981 url(banniere_matis.png) no-repeat scroll center;
  width: 900px;
  height: 160px;
}

#rap {
  margin: 0 auto;
  padding: 0;
  background: #ffffff none repeat scroll 0% 50%;
  width: 900px;
  text-align: left;
  font-size: 90%;
}

#content {
  margin: 0px;
  padding: 0 20px 0 10px;
  background: #ffffff none repeat scroll 0% 50%;
  float: right;
  width: 870px;
  color: #333333;
  min-height: 600px;
  text-align: left;
}
div.subindex {
  margin: 0em 0em 1em;
  background-position: left top;
  background-repeat: no-repeat;
  min-height: 0px;
  padding-left: 0px;
}
#subindex h2 {
  margin: 0em 0em 1em 5em;
  padding-left: 0px;
}

#footer {
  border-top: 1px solid #ddcccc;
  margin: 0 auto;
  padding: 5px 0;
  background: #ffffff none repeat scroll 0% 50%;
  clear: both;
  width: 900px;
  text-align: center;
  font-size: 95%;
}
#footer p {
  padding: 10px 0;
  text-align: center;
}
#content h2, #content h3 {
  border-bottom: thin solid #ebbc48;
  color: #800000;
  font-weight: normal;
  text-transform: uppercase;
  font-size: 1.6em;
}
#content h4 {
  border-bottom: thin none #ebbc48;
  color: #800000;
  font-weight: normal;
  text-transform: uppercase;
  font-size: 1.1em;
  text-decoration: underline;
}
#content h5 {
  font-family: Georgia,Geneva,Arial,times,serif;
  letter-spacing: 0.09em;
  line-height: 1.6em;
  color: #008000;
}
#content .post {
  padding-bottom: 2em;
}
#content p {
  margin: 0 0 10px;
  line-height: 1.7em;
  text-align: justify;
}
#content .fragment{
       font-family: monospace, fixed;
       font-size: 95%;
}
#content PRE.fragment {
	border: 1px solid #CCCCCC;
	background-color: #f5f5f5;
	margin-top: 4px;
	margin-bottom: 4px;
	margin-left: 2px;
	margin-right: 8px;
	padding-left: 6px;
	padding-right: 6px;
	padding-top: 4px;
	padding-bottom: 4px;
}
.posted {
  margin: 15px 0;
  padding: 5px;
  background: transparent url(img/topbg.gif) repeat-x scroll center top;
  color: #666666;
}
hr {
  display: none;
}
input.textbox, textarea {
  border: 1px solid #cccccc;
  background: #f2f2e9 none repeat scroll 0% 50%;
}
textarea {
  width: 80%;
}
#header, #content, #sidebar, #footer, .widget {
  overflow: hidden;
}
